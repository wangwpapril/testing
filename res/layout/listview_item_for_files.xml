<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="60dip" android:background="#FFFFFF"
	android:gravity="center_vertical">
	<ImageView android:id="@+id/imageViewfile_FileMLI"
		android:src="@drawable/phone_filemanager_folder"
		android:layout_alignParentLeft="true" android:layout_marginRight="15dip"
		android:layout_width="60dip" android:layout_height="60dip"></ImageView>
	<RelativeLayout android:layout_height="wrap_content"
		android:id="@+id/relativeLayoutContent_FileMLI" android:paddingLeft="5dip"
		android:paddingRight="5dip" android:layout_toRightOf="@+id/imageViewfile_FileMLI"
		android:layout_width="match_parent">
		<TextView android:layout_height="wrap_content" android:id="@+id/textViewTitle_FileMLI"
			android:text="asdsdda" android:layout_width="wrap_content"
			android:layout_alignParentTop="true" android:textColor="#000000"
			android:textSize="20dip" android:layout_marginTop="10dip"></TextView>
		<RelativeLayout android:layout_height="wrap_content"
			android:id="@+id/relativeLayoutContent_FileMLI" android:layout_below="@+id/textViewTitle_FileMLI"
			android:layout_width="match_parent" android:layout_alignParentBottom="true"
			android:gravity="center_vertical">
			<TextView android:layout_height="wrap_content" android:id="@+id/textViewCount_FileMLI"
				android:text="" android:layout_width="wrap_content"></TextView>
			<TextView android:layout_height="wrap_content" android:id="@+id/textViewTime_FileMLI"
				android:text="" android:layout_width="wrap_content"
				android:layout_alignParentRight="true"></TextView>
		</RelativeLayout>
	</RelativeLayout>
</RelativeLayout>