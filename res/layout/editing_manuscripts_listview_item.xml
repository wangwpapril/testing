<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="wrap_content" android:background="#FFFFFF"
	android:layout_marginTop="20dip">
	<ImageView android:layout_height="wrap_content"
		android:id="@+id/imageViewCheck_EditMAItem" android:src="@drawable/phone_draftstate_editingdraft_editcheck"
		android:layout_width="wrap_content" android:layout_alignParentLeft="true"
		android:layout_marginRight="15dip" android:layout_centerVertical="true"
		android:visibility="gone"></ImageView>
	<RelativeLayout android:layout_height="wrap_content"
		style="@style/manuscript_list_item_background" android:id="@+id/linearLayout1_EditMAItem"
		android:layout_width="match_parent" android:layout_toRightOf="@+id/imageViewCheck_EditMAItem">
		<RelativeLayout android:layout_height="wrap_content"
			android:layout_marginTop="10dip" android:id="@+id/relativeLayoutContent_EditMAItem"
			android:paddingLeft="5dip" android:paddingRight="5dip"
			android:layout_width="wrap_content" android:layout_toLeftOf="@+id/imageViewTitle_EditMAItem"
			android:layout_alignParentLeft="true">
			<TextView android:layout_height="wrap_content"
				android:id="@+id/textViewTitle_EditMAItem" android:text="asdsdda"
				android:layout_width="wrap_content" android:layout_alignParentTop="true"
				android:layout_alignParentLeft="true" style="@style/manuscript_list_item_Title"></TextView>
			<TextView android:layout_height="wrap_content"
				android:id="@+id/textViewTime_EditMAItem" android:text="3:12 8.9"
				android:layout_alignBottom="@+id/textViewTitle_EditMAItem"
				android:layout_width="wrap_content" android:layout_marginLeft="5dip"
				android:layout_toRightOf="@+id/textViewTitle_EditMAItem" style="@style/manuscript_list_item_Time"></TextView>
			<TextView android:layout_height="wrap_content"
				style="@style/manuscript_list_item_Content" android:id="@+id/textViewContent_EditMAItem"
				android:text="asds" android:layout_width="wrap_content"
				android:layout_below="@+id/textViewTime_EditMAItem"
				android:layout_marginTop="10dip"></TextView>
		</RelativeLayout>
		<ImageView android:id="@+id/imageViewTitle_EditMAItem"
			android:layout_height="70dip" android:layout_width="100dip"
			android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
	</RelativeLayout>
</RelativeLayout>
