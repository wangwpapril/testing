<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:background="#00000000"
	android:layout_height="wrap_content">
	<ImageView android:layout_width="100dip" android:id="@+id/imageViewItem_CA"
		android:layout_height="100dip" android:layout_centerHorizontal="true"></ImageView>
	<TextView android:layout_width="wrap_content"
		android:layout_below="@+id/imageViewItem_CA" android:layout_height="wrap_content"
		android:text="" android:layout_centerHorizontal="true"
		android:id="@+id/textViewItemTitle_CA">
	</TextView>
	<ImageButton android:layout_width="30dip"
		android:id="@+id/imageViewItemChoose_CA" android:layout_height="30dip"
		android:layout_alignParentRight="true" android:layout_alignParentTop="true"
		android:background="@null"
		android:src="@drawable/phone_draftlabel_choose"></ImageButton>
</RelativeLayout>
